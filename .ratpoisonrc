exec xsetroot -cursor_name left_ptr
exec xmodmap -e "keycode 133 = F19"
escape F19
bind c exec urxvt
exec rpws -1
exec /usr/bin/rpws init 6 -k
bind F1 exec rpws 1
bind F2 exec rpws 2
bind F3 exec rpws 3
bind F4 exec rpws 4
definekey top M-F1 exec rpws 1
definekey top M-F2 exec rpws 2
definekey top M-F3 exec rpws 3
definekey top M-F4 exec rpws 4
definekey top M-b banish
definekey top M-r resize
definekey top M-R remove
definekey top M-u undo
definekey top M-U redo
definekey top M-p exec
definekey top M-h focusleft
definekey top M-l focusright
definekey top M-j focusdown
definekey top M-k focusup
definekey top M-C-h exchangeleft
definekey top M-C-j exchangedown
definekey top M-C-k exchangeup
definekey top M-C-l exchangeright
definekey top M-i next
definekey top M-I prev
definekey top M-c exec urxvt
definekey top M-s split
definekey top M-f hsplit
definekey top M-o only
set wingravity n
set padding 0 11 0 0
set barborder 5
set border 1
set fwcolor #000000
set bwcolor #eee8d5
set fgcolor #073642
set bgcolor #eee8d5
set bargravity c
unmanage panel
exec conky | dzen2 -ta c -bg "#eee8d5" -fg "#000000" -x 0 -y 0 -h 10 -fn "-*-fixed-*-*-*-*-9-*-*-*-*-*-iso8859-*"
exec feh --bg-center solfond.png
exec rpws 1
split 3/5
focusdown
hsplit 1/2
focusup
