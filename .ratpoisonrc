exec xmodmap -e "keycode 115 = F19"
escape F19
bind c exec urxvt
definekey top M-b banish
definekey top M-r resize
definekey top M-R remove
definekey top M-u undo
definekey top M-U redo
definekey top M-p exec
definekey top M-h focusleft
definekey top M-l focusright
definekey top M-j focusdown
definekey top M-k focusup
definekey top M-C-h exchangeleft
definekey top M-C-j exchangedown
definekey top M-C-k exchangeup
definekey top M-C-l exchangeright
definekey top M-i next
definekey top M-I prev
definekey top M-c exec urxvt
definekey top M-s split
definekey top M-f hsplit
definekey top M-o only
definekey top M-n nextscreen
definekey top M-N prevscreen
set wingravity c
set barborder 1
set border 0
set bwcolor #151515
set fwcolor #999399
set bgcolor #000000
set fgcolor #afafaf
set bargravity c
exec xsetroot -cursor_name left_ptr
