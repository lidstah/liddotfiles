Xft.dpi: 96
*background:  #000000
*foreground:  #e3e0d7

! black + red
! !urxvt*color0:     #3f3f3f
*color0:      #1E2320
*color1:      #705050
! ! green + yellow
*color2:      #60b48a
*color3:      #dfaf8f
! ! blue + purple
*color4:      #506070
*color5:      #dc8cc3
! ! cyan + white
*color6:      #8cd0d3
*color7:      #dcdccc
! ! bright-black + bright-red
*color8:      #709080
*color9:      #dca3a3
! ! bright-green + bright-yellow
*color10:     #c3bf9f
*color11:     #f0dfaf
! ! bright-blue + bright-purple
*color12:     #94bff3
*color13:     #ec93d3
! ! bright-cyan + bright-white
*color14:     #94bff3
*color15:     #ffffff

!urxvt.font: xft:Bitstream Vera Sans Mono-7.5
!urxvt.font: xft:Monospace:size=8
!urxvt.font: xft:Lemon
urxvt.font: -*-terminus-medium-*-*-*-12-*-*-*-*-*-*-*
urxvt.termName:  rxvt-256color
urxvt.loginShell:         true
urxvt.scrollBar:         false
urxvt.secondaryScroll:    true
urxvt.saveLines:         1024
urxvt.cursorBlink:       false
urxvt.urgentOnBell:       true
urxvt.override-redirect: false
!urxvt.borderLess:       false
!urxvt.borderWidth:          1
!urxvt.internalBorder:       0
!
! Extensions
URxvt.perl-ext-common: default,url-select
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: /usr/bin/firefox
URxvt.url-select.underline: true

! Cursor
URxvt.borderless: true
URxvt.internalBorder: 3
URxvt.externalBorder: 3

